<h1>F L A S H E D</h1>

<% posts.forEach(p => { %>
<div class="row">
  <div class="col s4 offset-s4">
    <div class="card">
      <div class="card-image">
        <img src="<%= p.picPost %>">
      </div>
      <div class="card-content">
        <p><%= p.caption %></p>
      </div>
      
    </div>
  </div>
</div>
<% }) %>