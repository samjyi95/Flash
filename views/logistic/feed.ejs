<h1>Welcome to the feed page</h1>

<% posts.forEach(p => { %>
<div class="row">
  <div class="col s12 m7">
    <div class="card">
      <div class="card-image">
        <img src="<%= p.picPost %>">
      </div>
      <div class="card-content">
        <p><%= p.caption %></p>
      </div>
      <div class="card-action">
        <a href="viewPic">This is a link</a>
      </div>
    </div>
  </div>
</div>
<% }) %>