<h1>All the users will be listed here</h1>

<% users.forEach(u => { %>
<div class="row">
    <div class="col s12 m7">
      <div class="card">
        <div class="card-image">
          <img src="<%= u.pic %> " style="width: 350; height: auto;">
          <span class="card-title"><%= u.firstname %> <%= u.lastname %>  </span>
        </div>
        <div class="card-content">
          <p><%= u.bio %> </p>
        </div>
        <div class="card-action">
          <a href="/profile/guest/<%= u.id %> ">This is a link</a>
        </div>
      </div>
    </div>
  </div>
  <% }) %>