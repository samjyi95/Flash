<!-- view page of another user as a GUEST -->
<div class="row">
    <div class="col s4">
        <img src="<%= userProfile.pic %>" alt="profile pic" style="height: 500px; width: auto;">
        <div>
            <p><strong>Name:</strong> <%= user.firstname %> <%= user.lastname %> </p>
        </div>
        <div>
            <p>
                <%= user.bio %>
            </p>
        </div>
    </div>
</div>